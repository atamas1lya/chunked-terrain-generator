{
  "name": "terrain-generator",
  "version": "2.0.0-beta.3.1",
  "description": "Create your worlds seamlessly",
  "main": "dist/index.umd.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/stackdumper/terrain-generator.git"
  },
  "bugs": {
    "url": "https://github.com/stackdumper/terrain-generator/issues",
    "email": "stackdumper@gmail.com"
  },
  "scripts": {
    "start": "npx parcel test/index.html --out-dir test/dist",
    "build": "npx microbundle build --strict --sourcemap false --name terrain-generator --format umd",
    "lint": "npx eslint . --ignore-path .gitignore",
    "lint:fix": "npx eslint . --ignore-path .gitignore --fix; exit 0;",
    "precommit": "lint-staged",
    "release": "npm run build && npm publish ."
  },
  "lint-staged": {
    "*.js": ["npm run lint:fix", "git add"]
  },
  "author": "stackdumper",
  "license": "MIT",
  "dependencies": {
    "noisejs": "^2.1.0"
  },
  "devDependencies": {
    "babel-eslint": "^8.2.2",
    "eslint": "^4.19.1",
    "eslint-config-airbnb-base": "^12.1.0",
    "eslint-plugin-import": "^2.9.0",
    "eslint-plugin-prettier": "^2.6.0",
    "microbundle": "^0.4.4",
    "parcel-bundler": "^1.6.2"
  }
}
